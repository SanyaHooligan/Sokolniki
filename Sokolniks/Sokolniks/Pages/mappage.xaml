        <Grid.RowDefinitions>
            <RowDefinition Height="513"/>
            <RowDefinition Height="1407"/>
        </Grid.RowDefinitions>

        <!--Кнопки и заголовок-->
        <Grid Grid.Row="0" Margin="64,275,57,0">
            <Button Content="НАЗАД" Width="107" Height="40" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="0,23,0,0" 
                        Style="{StaticResource MapNavigationButtons}" Background="White" Command="{Binding GoBackCommand}"
                        DataContext="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=Page}}"/>

            <TextBlock Text="КАРТА" Foreground="#000000" FontSize="60" VerticalAlignment="Top" HorizontalAlignment="Center" FontFamily="../Fonts/#Stolzl Display"/>

            <Button Content="ПОИСК" Width="110" Height="40" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,23,0,0" 
                        Style="{StaticResource MapNavigationButtons}" Background="White" Click="Button_Click"/>
        </Grid>

