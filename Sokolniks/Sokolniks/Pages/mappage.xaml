        <!--Окно поиска-->
        <Grid Name="SearchSpace" Grid.RowSpan="3" Margin="0,1920,0,0" Background="White">
            <Grid.RowDefinitions>
                <RowDefinition Height="200"/>
                <RowDefinition Name="SecondRowSearch" Height="1265"/>
            </Grid.RowDefinitions>
            <!--Кнопки и надпись-->
            <Grid Grid.Row="0" Margin="64,0,57,0">
                <Button Content="НАЗАД" Width="107" Height="40" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="0,23,0,0" 
                        Style="{StaticResource MapNavigationButtons}" Background="White" Command="{Binding GoBackCommand}"
                        DataContext="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=Page}}"/>

                <TextBlock Text="ПОИСК" Foreground="#000000" FontSize="60" VerticalAlignment="Top" HorizontalAlignment="Center" FontFamily="../Fonts/#Stolzl Display"/>

                <Button Content="КАРТА" Width="110" Height="40" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,23,0,0" 
                        Style="{StaticResource MapNavigationButtons}" Background="White" Click="Button_Click_1"/>
            </Grid>
            <!--Поле ввода и результаты/предложка-->
            <Grid Grid.Row="1" Margin="64,0,57,0">
                <Grid Height="Auto">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <TextBox Width="959" Height="100" VerticalAlignment="Top"
                             TextWrapping="NoWrap" FontSize="70" Foreground="#000000" FontFamily="../Fonts/#Stolzl Display"
                             Panel.ZIndex="1"/>
                    <!--<TextBox Width="959" Height="100" VerticalAlignment="Top" Style="{StaticResource MapSearchfieldTextBox}"
                             TextWrapping="NoWrap" FontSize="70" Foreground="#000000" FontFamily="../Fonts/#Stolzl Display"
                             Text="{Binding TextSearch , Mode=TwoWay , UpdateSourceTrigger=PropertyChanged}" Panel.ZIndex="1"/>-->
                    <TabControl x:Name="products" Grid.Row="1" Margin="0,20,0,0" BorderBrush="White">
                        <TabItem x:Name="Category">
                            <TabItem.Template>
                                <ControlTemplate TargetType="{x:Type TabItem}">
                                    <Grid>
                                        <Border 
                                        Name="Border"
                                        Margin="0,0,44,0" 
                                        Background="White"
                                       BorderBrush="White"
                                        BorderThickness="0,0,0,4" 
                                        CornerRadius="2,12,0,0" >
                                            <ContentPresenter x:Name="ContentSite"
                                          VerticalAlignment="Center"
                                          HorizontalAlignment="Center"
                                          ContentSource="Header"
                                          Margin="12,2,12,2"
                                          RecognizesAccessKey="True"/>
                                        </Border>
                                    </Grid>
                                    <ControlTemplate.Triggers>
                                        <Trigger Property="IsSelected" Value="True">
                                            <Setter Property="Panel.ZIndex" Value="100" />
                                            <Setter TargetName="Border" Property="BorderBrush" Value="Gray" />
                                        </Trigger>
                                        <Trigger Property="IsEnabled" Value="False">
                                            <Setter TargetName="Border" Property="BorderBrush" Value="White" />
                                        </Trigger>
                                    </ControlTemplate.Triggers>
                                </ControlTemplate>
                            </TabItem.Template>
                            <TabItem.Header>
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Height="50" FontSize="40" Margin="3">Категории</TextBlock>
                                </StackPanel>
                            </TabItem.Header>
                            <TabItem.Content>
                                <ListBox Name="FixedCategoryListBox" SelectionChanged="FixedCategory_SelectionChanged" Grid.Row="1" Panel.ZIndex="1" BorderThickness="0" ItemsSource="{Binding AllCategories}" Margin="0,40,0,0" >
                                    <!--ItemContainerStyle="{StaticResource ListBoxItemsTemplate}"-->
                                    <ListBox.ItemsPanel>
                                        <ItemsPanelTemplate>
                                            <StackPanel>
                                            </StackPanel>
                                        </ItemsPanelTemplate>
                                    </ListBox.ItemsPanel>
                                    <ListBox.ItemTemplate>
                                        <DataTemplate>
                                            <Grid Margin="10">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition/>
                                                    <RowDefinition/>
                                                </Grid.RowDefinitions>
                                                <TextBlock FontFamily="../Fonts/#Stolzl Display" TextWrapping="Wrap" FontSize="40" Text="{Binding Name }"></TextBlock>
                                                <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Right">
                                                    <TextBlock Foreground="Gray" Text="мест: " FontSize="25"></TextBlock>
                                                    <TextBlock Foreground="Gray" Text="{Binding count}" FontSize="25"></TextBlock>
                                                </StackPanel>
                                            </Grid>
                                        </DataTemplate>
                                    </ListBox.ItemTemplate>
                                </ListBox>
                            </TabItem.Content>
                        </TabItem>
                        <TabItem x:Name="Popular">
                            <TabItem.Template>
                                <ControlTemplate TargetType="{x:Type TabItem}">
                                    <Grid>
                                        <Border 
                                        Name="Border"
                                        Margin="0,0,-4,0" 
                                        Background="White"
                                       BorderBrush="White"
                                        BorderThickness="0,0,0,4" 
                                        CornerRadius="2,12,0,0" >
                                            <ContentPresenter x:Name="ContentSite"
                                          VerticalAlignment="Center"
                                          HorizontalAlignment="Center"
                                          ContentSource="Header"
                                          Margin="12,2,12,2"
                                          RecognizesAccessKey="True"/>
                                        </Border>
                                    </Grid>
                                    <ControlTemplate.Triggers>
                                        <Trigger Property="IsSelected" Value="True">
                                            <Setter TargetName="Border" Property="BorderBrush" Value="Gray" />
                                        </Trigger>
                                        <Trigger Property="IsEnabled" Value="False">
                                            <Setter TargetName="Border" Property="BorderBrush" Value="White" />
                                        </Trigger>
                                    </ControlTemplate.Triggers>
                                </ControlTemplate>
                            </TabItem.Template>
                            <TabItem.Header>
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Height="50" FontSize="40"  Margin="3">Популярное</TextBlock>
                                </StackPanel>
                            </TabItem.Header>
                            <TabItem.Content>
                                <ListBox Name="PopularListBox" SelectionChanged="Popular_SelectionChanged" Grid.Row="1" Panel.ZIndex="1" BorderThickness="0" ItemsSource="{Binding PopularCategorise}" Margin="0,40,0,0" >
                                    <!--ItemContainerStyle="{StaticResource ListBoxItemsTemplate}"-->
                                    <ListBox.ItemsPanel>
                                        <ItemsPanelTemplate>
                                            <StackPanel>
                                            </StackPanel>
                                        </ItemsPanelTemplate>
                                    </ListBox.ItemsPanel>
                                    <ListBox.ItemTemplate>
                                        <DataTemplate>
                                            <Grid Margin="10">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition/>
                                                    <RowDefinition/>
                                                </Grid.RowDefinitions>
                                                <TextBlock FontFamily="../Fonts/#Stolzl Display" TextWrapping="Wrap" FontSize="40" Text="{Binding Name }"></TextBlock>
                                                <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Right">
                                                    <TextBlock Foreground="Gray" Text="мест: " FontSize="25"></TextBlock>
                                                    <TextBlock Foreground="Gray" Text="{Binding count}" FontSize="25"></TextBlock>
                                                </StackPanel>
                                            </Grid>
                                        </DataTemplate>
                                    </ListBox.ItemTemplate>
                                </ListBox>
                            </TabItem.Content>
                        </TabItem>
                    </TabControl>
                </Grid>
                <Grid Name="listGrid" Height="Auto" Margin="0,93,0,0" VerticalAlignment="Top">
                    <ScrollViewer VerticalAlignment="Top" Background="White" PanningMode="VerticalOnly" BorderBrush="Black" BorderThickness="0,0,0,1" Panel.ZIndex="2">
                        <StackPanel Background="White" Height="Auto" VerticalAlignment="Top">
                            <ListBox SelectionChanged="ListCategories_SelectionChanged" Name="ListCategories" ScrollViewer.VerticalScrollBarVisibility="Disabled" Background="White" Width="950"  Height="Auto" Panel.ZIndex="10"  VerticalAlignment="Top" BorderThickness="0" BorderBrush="Black" Style="{DynamicResource ListBoxStyle}" ItemsSource="{Binding CategorisForSearch,UpdateSourceTrigger=PropertyChanged}" >
                                <ListBox.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <StackPanel>
                                        </StackPanel>
                                    </ItemsPanelTemplate>
                                </ListBox.ItemsPanel>
                                <ListBox.ItemTemplate>
                                    <DataTemplate>
                                        <Grid Margin="10">
                                            <Grid.RowDefinitions>
                                                <RowDefinition/>
                                                <RowDefinition/>
                                            </Grid.RowDefinitions>
                                            <TextBlock FontFamily="../Fonts/#Stolzl Display" TextWrapping="Wrap" FontSize="40" Text="{Binding Name }"></TextBlock>
                                            <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Right">
                                                <TextBlock Foreground="Gray" Text="мест: " FontSize="25"></TextBlock>
                                                <TextBlock Foreground="Gray" Text="{Binding count}" FontSize="25"></TextBlock>
                                            </StackPanel>
                                        </Grid>
                                    </DataTemplate>
                                </ListBox.ItemTemplate>
                            </ListBox>

                            <ListBox SelectionChanged="ListMarks_SelectionChanged" Name="ListMarks" Width="950" ScrollViewer.VerticalScrollBarVisibility="Disabled" Height="Auto" Panel.ZIndex="2"  VerticalAlignment="Top" BorderThickness="0" BorderBrush="Black" Style="{DynamicResource ListBoxStyle}" ItemsSource="{Binding MarksForSearch,UpdateSourceTrigger=PropertyChanged}" >
                                <ListBox.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <StackPanel>
                                        </StackPanel>
                                    </ItemsPanelTemplate>
                                </ListBox.ItemsPanel>
                                <ListBox.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock Margin="10,30" Width="920" TextWrapping="Wrap" FontFamily="../Fonts/#Stolzl Display" FontSize="40" Text="{Binding MarkName }"></TextBlock>
                                    </DataTemplate>
                                </ListBox.ItemTemplate>
                            </ListBox>
                        </StackPanel>
                    </ScrollViewer>
                </Grid>
            </Grid>
        </Grid>

